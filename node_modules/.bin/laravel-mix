#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/pb/Documents/other/Developer/lampDockerDesktop/wwwlaravel/node_modules/.pnpm/laravel-mix@6.0.49_@babel+core@7.24.7_@babel+plugin-proposal-object-rest-spread@7.20.7_@babel_v2fzpd6h5oh72ngb26fuxf6rim/node_modules/laravel-mix/bin/node_modules:/Users/pb/Documents/other/Developer/lampDockerDesktop/wwwlaravel/node_modules/.pnpm/laravel-mix@6.0.49_@babel+core@7.24.7_@babel+plugin-proposal-object-rest-spread@7.20.7_@babel_v2fzpd6h5oh72ngb26fuxf6rim/node_modules/laravel-mix/node_modules:/Users/pb/Documents/other/Developer/lampDockerDesktop/wwwlaravel/node_modules/.pnpm/laravel-mix@6.0.49_@babel+core@7.24.7_@babel+plugin-proposal-object-rest-spread@7.20.7_@babel_v2fzpd6h5oh72ngb26fuxf6rim/node_modules:/Users/pb/Documents/other/Developer/lampDockerDesktop/wwwlaravel/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/pb/Documents/other/Developer/lampDockerDesktop/wwwlaravel/node_modules/.pnpm/laravel-mix@6.0.49_@babel+core@7.24.7_@babel+plugin-proposal-object-rest-spread@7.20.7_@babel_v2fzpd6h5oh72ngb26fuxf6rim/node_modules/laravel-mix/bin/node_modules:/Users/pb/Documents/other/Developer/lampDockerDesktop/wwwlaravel/node_modules/.pnpm/laravel-mix@6.0.49_@babel+core@7.24.7_@babel+plugin-proposal-object-rest-spread@7.20.7_@babel_v2fzpd6h5oh72ngb26fuxf6rim/node_modules/laravel-mix/node_modules:/Users/pb/Documents/other/Developer/lampDockerDesktop/wwwlaravel/node_modules/.pnpm/laravel-mix@6.0.49_@babel+core@7.24.7_@babel+plugin-proposal-object-rest-spread@7.20.7_@babel_v2fzpd6h5oh72ngb26fuxf6rim/node_modules:/Users/pb/Documents/other/Developer/lampDockerDesktop/wwwlaravel/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../laravel-mix/bin/cli.js" "$@"
else
  exec node  "$basedir/../laravel-mix/bin/cli.js" "$@"
fi
